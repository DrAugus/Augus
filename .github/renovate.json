{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "packageNames": [
        "vuepress",
        "@vuepress/client",
        "@vuepress/utils",
        "@vuepress/plugin-google-analytics"
      ],
      "allowedVersions": "!/pre.*$/"
    }
  ]
}